<div id="edit_blurb" class="modal hide fade">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h3>Edit Blurb</h3>
  </div>
  <div class="modal-body">
    <input type="hidden" name="id" id="id" value="<%= @campaign.id %>" />
    <textarea name="blurb" id="blurb" rows="8" class="blurb_box input-xlarge"></textarea>
  </div>
  <div class="modal-footer">
    <button href="#" class="btn btn-primary save" type="submit">Save</button>
  </div>
</div>

<div id="add_faq" class="modal hide fade">
    <%= form_for :campaign_faq, :remote => true, :url => "/campaigns/add-faq" do |f| %>
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
      <h3>New FAQ</h3>
    </div>
    <div class="modal-body">
      <input type="hidden" name="id" id="id" value="<%= @campaign.id %>" />
      <label for="question">Question</label>
      <input name="question" id="question" type="text" />
      <label for="question">Answer</label>
      <textarea name="answer" id="answer" rows="8" class="input-xlarge"></textarea>
    </div>
    <div class="modal-footer">
      <button href="#" class="btn btn-primary save" type="submit">Save</button>
    </div>
    <% end %>
</div>

<div id="add_level" class="modal hide fade">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h3>New Level</h3>
  </div>
  <div class="modal-body">
    <input type="hidden" name="id" id="id" value="<%= @campaign.id %>" />
    <textarea name="level" id="level" rows="8" class="level_box input-xlarge"></textarea>
  </div>
  <div class="modal-footer">
    <button href="#" class="btn btn-primary save" type="submit">Save</button>
  </div>
</div>

<div id="add_section" class="modal hide fade">
